{
  "name": "ModHeader fork",
  "version": "2.3.2",
  "manifest_version": 3,
  "description": "Modify request and response headers",
  "background":  {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Modify Headers",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "incognito": "spanning",
  "permissions": [
    "contextMenus",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
